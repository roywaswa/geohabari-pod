  "usesStyledComponents": false,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": true
    },
    "section": {
      "path": "src/sections",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": true
    },
    "page": {
